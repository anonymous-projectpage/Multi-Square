{
    "alg_name":"multi_rl",
    "benchmark":"alfworld",
    "max_new_tokens":16,
    "do_sample":true,
    "temperature":0.7,
    "use_lora":true,
    "use_lora_critic": true,
    "use_lora_high": true,      
    "use_lora_low": true,  
    "low_update_repeat": 5,
    "model_name": "meta-llama/Llama-3.1-8B-Instruct",
    "log_path":"./logs",
    "check_path":"./check_points",
    "ds_config":"./config/zero2.json",
    "env_step_limit":50,
    "train_micro_batch_size_per_gpu": 4,
    "eval_freq":1000,
    "log_freq": 10,
    "gamma":0.99,
    "epochs":400,
    "seed":22,
    "actor_lr":0.0001,
    "critic_lr":1e-05,
    "lr_warmup": 0.0005, 
    "tau":0.2,
    "weight_tau":0.5,
    "beta": 10,
    "awbc_w_clip":20,
    "lambda_adv": 7,
    "relu_adv_weight": true,
    "normalize_w":true,
    "use_policy_action": true,
    "mode":"bc",
    "use_adv":true
